// This is going to be the base config file for Monitord
// Comments are c++ stylee 
// 
<output>
printToScreen#I1=1;
verbosity#I1=2;
</output>


<monitord>
ramdiskKillAcqd#I1=200;
maxAcqdWaitPeriod#I1=180;
inodesKillAcqd#I1=10000;
inodesDumpData#I1=2000;
ramdiskDumpData#I1=100;
startProcesses#I1=0;
monitorPeriod#I1=30; //In Seconds
usbSwitchMB#I1=50; // In Megabytes
satabladeSwitchMB#I1=120; // In Megabytes
sataminiSwitchMB#I1=120; // In Megabytes
maxEventQueueSize=600; 
maxHkQueueSize=300; 
</monitord>

